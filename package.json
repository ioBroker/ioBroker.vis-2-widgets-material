  "keywords": [
    "ioBroker",
    "weather",
    "home automation",
    "vis-2-widgets-material"
  ],
  "devDependencies": {
    "@alcalzone/release-script": "^3.5.9",
    "@alcalzone/release-script-plugin-iobroker": "^3.5.9",
    "@alcalzone/release-script-plugin-license": "^3.5.9",
    "@iobroker/vis-2-widgets-react-dev": "^0.4.2",
    "@types/gulp": "^4.0.11",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.2",
    "eslint": "^8.43.0",
    "gulp": "^4.0.2",
    "mocha": "^10.2.0",
    "@iobroker/vis-2-widgets-testing": "^0.2.7"
  },
  "main": "main.js",
  "files": [
    "admin/",
    "img/",
    "io-package.json",
    "LICENSE",
    "widgets/"
